snippet str
	typedef struct ${1:name}_struct
	{
	  ${2}
	}
	$1;
snippet iv
	internal void
snippet iiv
	internal inline void
snippet -
	${1}(${2})
	{
		${3}
	}
snippet def
	#define 
snippet inc
	#include <${1}>
snippet ifd
	#ifdef ${1}
		${2}
	#endif
snippet ifnd
	#ifndef ${1}
		${2}
	#endif
snippet do
	do {
		${1}
	} while (0)
